
<p class = "NewUsers-title"> Sign-in!</p>
<section class = "UsersNew" >
  <%= form_with url: new_sessions_path do |form| %>
    <div  class= "Form-section">
      <p class = " FormInput-label"> Email:
      <%= form.email_field :email, class: "Form_input", autofocus: true %> </p>
     </div>
    <div  class= "Form-section">
      <p class = " FormInput-label"> Password:
      <%= form.password_field :password, class: "Form_input" %> </p>
     </div>
     <div class = "Form-section">
      <%= form.submit "Sign-in!", class: "Form-submit" %>
  <% end %>
</section>
